<div class="container individual-consultory-container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
    <div class="row individual-tab-navigation">
      <hr />
      <ul class="nav nav-pills" id="individual-pills">
        <li class="active">
          <a href="#personal" id="individual-personal-link" data-toggle="tab" data-target=".personal, #personal-header">
          <%= t('static_pages.individual_consultory_body.pills.personal.title') %></a>
          <p><%= t('static_pages.individual_consultory_body.pills.personal.subtitle') %></p>
        </li>
        <li>
          <a href="#consultory" id="individual-consultory-link" data-toggle="tab" data-target=".consultory, #consultory-header">
          <%= t('static_pages.individual_consultory_body.pills.consulting.title') %></a>
          <p><%= t('static_pages.individual_consultory_body.pills.consulting.subtitle') %></p>
        </li>
        <li>
          <a href="#training" id="individual-training-link" data-toggle="tab" data-target=".training, #training-header">
          <%= t('static_pages.individual_consultory_body.pills.training.title') %></a>
          <p><%= t('static_pages.individual_consultory_body.pills.training.subtitle') %></p>
        </li>
        <!-- li>
          <a href="#intensive" id="individual-intensive-link" data-toggle="tab" data-target=".intensive, #intensive-header">INTENSIVE TEACHING</a>
          <p>2 d√≠as</p>
        </li -->
      </ul>
      <hr />
      <!-- PERSONAL -->
      <div class="tab-content">
        <div id="personal" class="tab-pane active personal">
          <div class="col-md-7 ind-body">
            <div>
             <div>
               <%= render 'individual_consultory_personal' %>
             </div>
            </div>
          </div>
          <div class="col-md-5 individual-sidebar">
            <div class="row">
              <div class="col-md-10 col-md-offset-1">
                <%= render 'individual_sidebar_criteria' %>
                <%= render 'individual_sidebar_peugeot_testimony' %>
                <%= render 'individual_sidebar_unique_method' %>
                <div class="our-consultors-programs-link">
                  <a href="contact">
                    <%= t('static_pages.individual_consultory_body.first_meeting') %>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      <!-- TRAINING -->
        <div  id="training" class="tab-pane training">
          <div class="col-md-7 ind-body">
            <div>
             <div>
               <%= render 'individual_consultory_training' %>
             </div>
            </div>
          </div>
          <div class="col-md-5 individual-sidebar">
            <div class="row">
              <div class="col-md-10 col-md-offset-1">
                <%= render 'individual_sidebar_criteria' %>
                <%= render 'individual_sidebar_nestle' %>
                <%= render 'individual_sidebar_unique_method' %>
              </div>
            </div>
          </div>
        </div>
      <!-- CONSULTORY -->
        <div  id="consultory" class="tab-pane consultory">
          <div class="col-md-7 ind-body">
            <div>
             <div>
               <%= render 'individual_consultory_consultory' %>
             </div>
            </div>
            <!-- div class="container">
              <div class="row our-consultors-programs-link">
                <a href="apply">
                  CANDIDATURA
                </a>
              </div>
            </div -->  
          </div>
          <div class="col-md-5 individual-sidebar">
            <div class="row">
              <div class="col-md-10 col-md-offset-1">
                <%= render 'individual_sidebar_criteria' %>
                <%= render 'individual_sidebar_experts' %>
                <%= render 'individual_sidebar_unique_method' %>
              </div>
            </div>
          </div>
        </div>
      <!-- INTENSIVE -->
        <div id="intensive" class="tab-pane intensive">
          <div class="col-md-7 ind-body">
            <div>
             <div>
               <%= render 'individual_consultory_intensive' %>
             </div>
            </div>
              <div class="our-consultors-programs-link">
                <a href="contact">
                  CANDIDATURA
                </a>
              </div>
          </div>
          <div class="col-md-5 individual-sidebar">
            <div class="row">
              <div class="col-md-10 col-md-offset-1">
                <%= render 'individual_sidebar_criteria' %>
                <%= render 'individual_sidebar_experts' %>
                <%= render 'individual_sidebar_unique_method' %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
  </div>
</div>